{
  "$id": "address.json",
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "required": ["street", "city", "zipcode", "country", "state", "additional"],
  "additionalProperties": false,
  "properties": {
    "street": { "type": "string" },
    "city": { "type": "string" },
    "zipcode": { "type": "string" },
    "country": { "type": "string" },
    "state": {"type": ["string", "null"] },
    "additional": {"type": ["string", "null"] }
  }
}